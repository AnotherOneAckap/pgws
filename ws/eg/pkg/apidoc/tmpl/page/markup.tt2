<script>
$(function() {
  $('#swatch-menu li a').bind('click',
    function(e) {
      if($(this).attr('rel') == 'default'){
	$("#switch_style").attr("href", "/css/style02/bootstrap.pgws.css");       
      }else{
	$("#switch_style").attr("href", "http://bootswatch.com/"+$(this).attr('rel')+"/bootstrap.min.css");       
      }
      return false;
    }
  );
});
</script>



