# Progress bars
# ==================================================
  h(type = 3, text = 'Progress bars', id = 'progressbars'); 

  row_begin;
    div_begin(width='low');
      progress(progress=60);

    div_end;
    div_begin(width='low');
      progress(class='progress-info progress-striped', progress=20);
